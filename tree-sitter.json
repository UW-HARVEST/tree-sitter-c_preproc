{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "c_preproc",
      "camelcase": "CPreproc",
      "title": "C Preprocessor",
      "scope": "source.preproc.c",
      "file-types": [
        "preproc.c"
      ],
      "injection-regex": "^c_preproc$",
      "class-name": "TreeSitterCPreproc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "C preprocessor grammar for tree-sitter",
    "authors": [
      {
        "name": "Haoran Peng",
        "email": "hrpeng@cs.washington.edu"
      }
    ],
    "links": {
      "repository": "https://gitlab.cs.washington.edu/harvest/tree-sitter-c_preproc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}